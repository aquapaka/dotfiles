  "default_container_padding": 4,
  "border": true,
  "border_width": 2,
  "border_style": "Square",
  "border_colours": {
    "single": "#000001",
    "stack": "#2785f2",
    "monocle": "#216609",
    "unfocused": "#F2EEDE"
  },
  "transparency": false,
