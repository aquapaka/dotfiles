{
  "default_workspace_padding": 4,
  "default_container_padding": 4,
  "border": true,
  "border_width": 1,
  "border_style": "Square",
  "border_colours": {
    "single": "#27fbfe",
    "stack": "#19bffe",
    "monocle": "#fb007a",
    "unfocused": "#070219"
  },
  "transparency": false,
  "stackbar": {
    "height": 40,
    "mode": "OnStack",
    "tabs": {
      "width": 300,
      "focused_text": "#27fbfe",
      "unfocused_text": "#6800d2",
      "background": "#070219"
    }
  }
}
